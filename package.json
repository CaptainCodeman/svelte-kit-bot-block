{
	"name": "svelte-kit-bot-block",
	"version": "0.0.8",
	"type": "module",
	"keywords": [
		"svelte-kit",
		"http",
		"bot",
		"block"
	],
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js"
		}
	},
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"homepage": "https://captaincodeman.github.io/svelte-kit-bot-block/",
	"repository": {
		"type": "git",
		"url": "https://github.com/captaincodeman/svelte-kit-bot-block.git"
	},
	"author": {
		"name": "Simon Green",
		"email": "simon@captaincodeman.com",
		"url": "https://www.captaincodeman.com/"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "tsup && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^25.2.0",
		"publint": "^0.3.17",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"tslib": "^2.8.1",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0"
	}
}